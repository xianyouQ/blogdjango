{% extends "blog/base.html" %}
{% block title %}
Blog
{% endblock %}
{% block jshead %}
	{{ block.super }}
	 <script>
	   $(document).ready(function(){
		   $("#myCarousel").carousel('cycle');
	   });
	</script>
{% block jshead %}
{% block content %}

<div id="myCarousel" class="leftcontent col-sm-12 col-md-9 carousel slide">
   <div class="carousel-inner">
   
      <div class="active item">
         <img src="/wp-content/uploads/2014/07/slide1.png" alt="First slide">
      </div>
      <div class="item">
         <img src="/wp-content/uploads/2014/07/slide2.png" alt="Second slide">
      </div>
      <div class="item">
         <img src="/wp-content/uploads/2014/07/slide3.png" alt="Third slide">
      </div>
   </div>
</div>
{ % comment %}
<div class="leftcontent col-sm-12 col-md-9">
	{% if error %}
		{{error}}
	{% else %}
		{% include "blog/ArticleSummary.html" %}
	{% endif %}
	}
</div>
{ % endcomment %}
<div class="rightcontent col-sm-0 col-md-3">
	<div class="bs-callout-info">所有分类</div>
	<div class="right_classic">
		{% include "rightNav.html" %}
	</div>
</div>
{% endblock %}


{% block footer %}

{% endblock %}